        <obo:MFOEM_000010 xml:lang="es">satisfacción</obo:MFOEM_000010>
        <obo:MFOEM_000010 xml:lang="de">zufriedenheit</obo:MFOEM_000010>
        <obo:MFOEM_000165>contented</obo:MFOEM_000165>
        <rdfs:label>contentment</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000048 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000048">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000199"/>
