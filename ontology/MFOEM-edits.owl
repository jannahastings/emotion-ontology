        <obo:MFOEM_000010 xml:lang="fr">amour</obo:MFOEM_000010>
        <obo:MFOEM_000010 xml:lang="de">liebe</obo:MFOEM_000010>
        <obo:MFOEM_000165>loving</obo:MFOEM_000165>
