    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/MFOEM_000049 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000049">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000048"/>
